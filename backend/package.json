{
  "name": "express-api",
  "version": "1.0.0",
  "description": "Express REST API",
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "main": "cluster.js",
  "scripts": {
    "start": "node cluster.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Quang Nguyen",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.0"
  },
  "devDependencies": {
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.1"
  }
}
